<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="330.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLResultsScreenController">
   <children>
      <TableView fx:id="TVResultsScreen" layoutX="30.0" layoutY="25.0" prefHeight="250.0" prefWidth="291.0">
        <columns>
          <TableColumn fx:id="TCSorters" prefWidth="115.0" text="Sorters" />
          <TableColumn fx:id="TCTime" prefWidth="115.0" text="Time" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="showResultsButton" layoutX="134.0" layoutY="291.0" mnemonicParsing="false" onAction="#showResultsButtonEvent" text="Show results" />
   </children>
</AnchorPane>
